{
  "name": "marketplace-rest-api",
  "version": "1.0.0",
  "description": "Marketpace Rest API",
  "main": "dist/cmd/main.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev:api:bsc": "ENTRY_POINT=api node --env-file=.prod.bsc.env -r ts-node/register -r tsconfig-paths/register src/cmd/main.ts",
    "dev:api:pol": "ENTRY_POINT=api node --env-file=.prod.pol.env -r ts-node/register -r tsconfig-paths/register src/cmd/main.ts",
    "dev:hero:bsc": "ENTRY_POINT=hero node --env-file=.prod.bsc.env -r ts-node/register -r tsconfig-paths/register src/cmd/main.ts",
    "dev:hero:pol": "ENTRY_POINT=hero node --env-file=.prod.pol.env -r ts-node/register -r tsconfig-paths/register src/cmd/main.ts",
    "dev:house:bsc": "ENTRY_POINT=house node --env-file=.prod.bsc.env -r ts-node/register -r tsconfig-paths/register src/cmd/main.ts",
    "dev:house:pol": "ENTRY_POINT=house node --env-file=.prod.pol.env -r ts-node/register -r tsconfig-paths/register src/cmd/main.ts"
  },
  "keywords": [
    "express",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.6",
    "dotenv": "^16.3.1",
    "ethers": "^6.16.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "winston": "^3.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^4.28.10",
    "@types/long": "^5.0.0",
    "@types/node": "^20.8.0",
    "@types/node-cron": "^3.0.8",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^3.2.4",
    "nodemon": "^3.0.1",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
